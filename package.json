{
  "name": "@interest-protocol/sdk-monorepo",
  "version": "0.0.1",
  "private": true,
  "description": "Interest Protocol SDK monorepo",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "clean": "pnpm -r clean",
    "build:all": "pnpm --filter \"@interest-protocol/*\" --stream build",
    "exec:script": "tsx ./scripts/set-up/exec-script.ts",
    "create:package": "tsx ./scripts/set-up/create-package.ts"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "scripts/*",
      "tools/*"
    ]
  },
  "keywords": [
    "sdk",
    "interest-protocol",
    "monorepo"
  ],
  "author": "Interest Protocol",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@eslint/js": "^9.26.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.15.18",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.2",
    "tiny-invariant": "^1.3.3",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "yargs": "^17.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/interest-protocol/sdk-monorepo.git"
  },
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=16.13.0"
  }
}
