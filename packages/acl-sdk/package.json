{
  "name": "@interest-protocol/acl-sdk",
  "author": "jose cerqueira",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/acl/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "version": "2.0.0",
  "description": "acl sdk",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "build:types": "tsc --project tsconfig.json",
    "build:tsup": "tsup './src/index.ts' --format esm,cjs --sourcemap",
    "build": "pnpm run clean && pnpm run build:tsup && pnpm run build:types",
    "dev": "tsc --watch"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@interest-protocol/prettier-config": "workspace:*",
    "@interest-protocol/typescript-config": "workspace:*",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5"
  },
  "dependencies": {
    "@interest-protocol/sui-core-sdk": "workspace:*",
    "@mysten/sui": "^1.26.0",
    "@polymedia/suitcase-core": "^0.0.36",
    "tiny-invariant": "^1.3.3"
  }
}
