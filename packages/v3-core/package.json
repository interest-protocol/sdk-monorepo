{
  "name": "@interest-protocol/v3-core",
  "author": "jose cerqueira",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "version": "0.1.0",
  "description": "v3-core package",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "build:types": "tsc --project tsconfig.json",
    "build:tsup": "tsup './src/index.ts' --format esm,cjs --sourcemap",
    "build": "pnpm run clean && pnpm run build:tsup && pnpm run build:types",
    "dev": "tsc --watch",
    "test": "jest --no-watchman",
    "test:watch": "jest --watch --no-watchman"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@interest-protocol/jest-config": "workspace:*",
    "@interest-protocol/prettier-config": "workspace:*",
    "@interest-protocol/typescript-config": "workspace:*",
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.3.6"
  },
  "dependencies": {
    "bignumber.js": "^9.3.0",
    "decimal.js": "^10.5.0",
    "tiny-invariant": "^1.3.3"
  }
}
