{
  "name": "@interest-protocol/farms",
  "author": "jose cerqueira",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "version": "1.2.0",
  "description": "farms package",
  "scripts": {
    "prebuild": "rm -rf dist",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "build:types": "tsc --project tsconfig.json",
    "build:tsup": "tsup './src/index.ts' --format esm,cjs --sourcemap",
    "build": "pnpm run clean && pnpm run build:tsup && pnpm run build:types",
    "dev": "tsc --watch",
    "test": "jest --no-watchman",
    "test:watch": "jest --watch --no-watchman"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@interest-protocol/jest-config": "workspace:*",
    "@interest-protocol/prettier-config": "workspace:*",
    "@interest-protocol/typescript-config": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/ramda": "^0.30.2",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1"
  },
  "dependencies": {
    "@interest-protocol/sui-core-sdk": "workspace:*",
    "@mysten/sui": "^1.45.2",
    "@polymedia/suitcase-core": "^0.0.67",
    "bignumber.js": "^9.3.1",
    "ramda": "^0.30.1",
    "tiny-invariant": "^1.3.3"
  },
  "publishConfig": {
    "access": "public",
    "dependencies": {
      "@interest-protocol/sui-core-sdk": "1.0.0",
      "@mysten/sui": "^1.36.2",
      "@polymedia/suitcase-core": "^0.0.67",
      "ramda": "^0.30.1",
      "tiny-invariant": "^1.3.3"
    }
  }
}
